HOST=0.0.0.0
PORT=8080
PREFIX=/api

SWAGGER_MODULE=true

STORE_DEST=/static/
STORE_FILES_COUNT=1
STORE_FILE_SIZE=5
STORE_MIME_TYPE=["image/jpeg", "image/jpg", "image/png"]

CORS_ORIGIN=["http://localhost:8080"]
CORS_CREDENTIALS=true

TYPEORM_NAME=example
TYPEORM_TYPE=postgres
TYPEORM_HOST=localhost
TYPEORM_PORT=5432
TYPEORM_CACHE=true
TYPEORM_LOGGING=all
TYPEORM_DATABASE=somedatabase
TYPEORM_USERNAME=dev
TYPEORM_PASSWORD=dev
TYPEORM_DROPSCHEMA=false
TYPEORM_KEEP_ALIVE=true
TYPEORM_SYNCHRONIZE=true
TYPEORM_MIGRATIONS_RUN=true
TYPEORM_ENTITIES=src/modules/**/*.entity.ts
TYPEORM_SUBSCRIBERS=src/modules/**/*.subscriber.ts
TYPEORM_MIGRATIONS_DIR=src/database/migrations/
